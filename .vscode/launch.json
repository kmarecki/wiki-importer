{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch cs",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/app.js",
			"stopOnEntry": false,
			"args": [
				"--xmlPath=../../wikibackup/cswiktionary-20160407-pages-articles-multistream.xml",
				"--outputDir=../../wikipages/cs2",
				"čeština",
				"polština",
				"angličtina",
				"ruština"
			],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": null
		},
		{
			"name": "Launch pl",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/app.js",
			"stopOnEntry": false,
			"args": [
				"--xmlPath=../../wikibackup/plwiktionary-20180101-pages-articles-multistream.xml",
				"--outputDir=../../wikipages/pl2",
				"--adapter=plwiktionary",
				"polski",
				"czeski",
				"angielski",
				"rosyjski"
				
			],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": null
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"outDir": null,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		},
		{
			"name": "Run mocha",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": [
				"tests/**/*.js"
			],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "test"
			},
			"sourceMaps": true,
			"outDir": null
		}
	]
}